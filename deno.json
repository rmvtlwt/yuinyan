{
	"deploy": {
		"include": [
			"main.ts"
		]
	},
	"imports": {
		"@discordjs/rest": "npm:@discordjs/rest",
		"discord_api_types": "https://deno.land/x/discord_api_types@0.37.67/v10.ts",
		"std": "https://deno.land/std@0.213.0"
	},
	"lint": {
		"rules": {
			"tags": [
				"recommended"
			],
			"include": [
				"ban-untagged-todo",
				"camelcase"
			]
		}
	},
	"fmt": {
		"semiColons": true,
		"useTabs": true,
		"indentWidth": 4
	},
	"tasks": {
		"build": "deno lint && deno check main.ts && deno fmt --check"
	}
}
